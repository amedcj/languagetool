<!-- Kurdish rule, 2022-07-29 -->
<rule id="CIN_KES" name="Cin_Kes">
 <pattern>
  <token>başim</token>
  <token>.</token>
 </pattern>
 <message>Cînavka kesadinê "im" ji rengdêran cuda tê nivîsandin.</message>
 <example correction=''>Ez <marker>başim.</marker></example>
 <example>Ez baş im.</example>
</rule>
<rule id="CIN_KES" name="Cin_Kes">
 <pattern>
  <token>başî</token>
  <token>.</token>
 </pattern>
 <message>Cînavka kesadinê "î" ji rengdêran cuda tê nivîsandin.</message>
 <example correction=''>Tu <marker>başî.</marker></example>
 <example>Tu baş î.</example>
</rule>
<rule id="CIN_KES" name="Cin_Kes">
 <pattern>
  <token>başe</token>
  <token>.</token>
 </pattern>
 <message>Cînavka kesadinê "e" ji rengdêran cuda tê nivîsandin.</message>
 <example correction=''>Ew <marker>başe.</marker></example>
 <example>Ew baş e.</example>
</rule>
<rule id="CIN_KES" name="Cin_Kes">
 <pattern>
  <token>başin</token>
  <token regexp='yes'></token>
 </pattern>
 <message>Cînavka kesadinê "in" ji rengdêran cuda tê nivîsandin.</message>
 <example correction=''>Ew <marker>başin.</marker></example>
 <example>Ew baş in.</example>
</rule>